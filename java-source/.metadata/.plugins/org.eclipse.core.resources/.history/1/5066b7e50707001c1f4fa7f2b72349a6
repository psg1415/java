package chapter8;

public class CustomerTest3 {
	public static void main(String[]args) {
		//배열 - 동일자료형의 연속된 공간
		
		Customer c1 = new Customer(1000, "고객1");
		Customer vc = new VIPCustomer2(1001, "고객2", 1000); //Customer 인스턴스, VIPCustomer2 인스턴스
		Customer gc = new GoldCustomer(1002, "고객3"); //Customer 인스턴스, GoldCustomer 인스턴스
		
	}
}
