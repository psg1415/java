package study;

public class Ex2 {	
	public static void main(String[]args) {
		int studentNum;
		int roomNum1;
		for(studentNum = 1; studentNum <= 40; studentNum++) {
			int studentNum1 = studentNum;
			}
		
		for(roomNum1 = 0; roomNum1 <= 9; roomNum1++) {
			int roomNum = studentNum % 10;			
		}
		System.out.println("학생" + studentNum + ", 방번호" + roomNum1 + "번");
	}
}
